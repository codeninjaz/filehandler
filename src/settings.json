{
  "style": {
    "height"          : "200px",
    "backgroundColor" : "#9BEEFF"
  },
  "maxFileSize"           : 102400,
  "allowedFileExtensions" : ["pdf", "doc", "jpg", "png", "gif"],
  "apiUrlBase"            : "http://localhost:8099/",
  "genericFileIcon"       : "file-o",
  "folderIcon"            : "folder-o",
  "openFolderIcon"        : "folder-open-o",
  "localStorageName"      : "fileListData",
  "rootIcon"              : "level-down",
  "infobox" : {
    "class" : "info-box",
    "style" : {
      "position"        : "absolute",
      "top"             : "8px",
      "left"            : "10px",
      "backgroundColor" : "#C5DB98",
      "zIndex"          : 10000,
      "padding"         : "10px",
      "border"          : "solid 1px #333",
      "boxShadow"       : "10px 10px 37px -2px rgba(165, 165, 165, 0.34)"
    }
  },
  "fileIcons":[
    {
      "types" :["png", "gif", "jpg"],
      "icon"  :"file-image-o",
      "color" :"#000"
    },
    {
      "types" :["pdf"],
      "icon"  :"file-pdf-o",
      "color" :"#FF161A"
    },
    {
      "types" :["zip", "lha", "gz", "7z"],
      "icon"  :"file-archive-o",
      "color" :"#000"
    },
    {
      "types" :["avi", "mwv", "mp4", "mov"],
      "icon"  :"file-video-o",
      "color" :"#000"
    },
    {
      "types" :["txt"],
      "icon"  :"file-text-o",
      "color" :"#000"
    },
    {
      "types" :["js", "cs", "html"],
      "icon"  :"file-code-o",
      "color" :"#000"
    },
    {
      "types" :["mp3", "aif"],
      "icon"  :"file-audio-o",
      "color" :"#000"
    },
    {
      "types" :["doc", "docx"],
      "icon"  :"file-word-o",
      "color" :"#143B8E"
    },
    {
      "types" :["xls", "xlsx"],
      "icon"  :"file-excel-o",
      "color" :"#5E9855"
    },
    {
      "types" :["ppt", "pptx"],
      "icon"  :"file-powerpoint-o",
      "color" :"#000"
    },
    {
      "types" :["link"],
      "icon"  :"link",
      "color" :"#A000B8"
    },
    {
      "types" :["default"],
      "icon"  :"file-o",
      "color" :"#000"
    }
  ],
  "toolbar" : {
    "editIcon"          : "pencil",
    "deleteIcon"        : "trash-o",
    "doneIcon"          : "check",
    "addLinkIcon"       : "link",
    "infoIcon"          : "info-circle",
    "createIcon"        : "plus-square",
    "buttonOnColor" : "#7DB61A"
  }
}
